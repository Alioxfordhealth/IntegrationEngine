<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="HPRIM">
		<xs:annotation>
			<xs:documentation>Racine schema Hprim Sante 2.1 et 2.2 Antares V2 tous contextes</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="H"/>
				<xs:element ref="P" maxOccurs="unbounded"/>
				<xs:element ref="L"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="H">
		<xs:annotation>
			<xs:documentation>Segment H : En-tête du message</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="H.2.1">
					<xs:annotation>
						<xs:documentation>Définition des séparateurs</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="H.3.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Identification du message (nom du fichier)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="H.4.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Mot de passe</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="H.5.1">
					<xs:annotation>
						<xs:documentation>Codification de l'emetteur</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="H.5.2">
					<xs:annotation>
						<xs:documentation>Nom de l'emetteur</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="H.6.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Ligne 1 adresse de l'emetteur</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="H.6.2" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Ligne 2 adresse de l'emetteur</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="H.6.3" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Ville de l'emetteur</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="H.6.4" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Libellé département de l'emetteur</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="H.6.5" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Code postal de l'emetteur</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="H.6.6" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Pays de l'emetteur</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="H.7.1">
					<xs:annotation>
						<xs:documentation>Type de message : ORU</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="H.8.1" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Numéro de téléphone, répétiteur autorisé</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="H.9.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Caractéristique de la transmission</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="H.10.1">
					<xs:annotation>
						<xs:documentation>Codification du récepteur</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="H.10.2">
					<xs:annotation>
						<xs:documentation>Nom du récepteur</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="H.11.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Commentaire</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="H.12.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Mode de traitement : P (production), T (test) ou D (debug)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="H.13.1">
					<xs:annotation>
						<xs:documentation>Version de la norme (H2.1 ou H2.2)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="H.13.2" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Type de liaison : L (liaison entre laboratoires), C (liaison  entre laboratoires et etablissements, R (liaison entre radiologie et etablissement)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="H.14.1">
					<xs:annotation>
						<xs:documentation>Date et heure de constitution du message</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="H.15.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Rajout pour compatibilité</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="C" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="P">
		<xs:annotation>
			<xs:documentation>Segment P : Patient</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="P.2.1">
					<xs:annotation>
						<xs:documentation>Rang du segment</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="P.3.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Identifiant du patient attribué par le demandeur (IPP généralement)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.3.2" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Identifiant d'un patient à fusionner</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.3.3" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Indicateur "FU" indiquant une demande de fusion de patient</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.4.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Identifiant du patient attribué par l'executant</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.4.2" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Compatibilité ERA</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.5.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Numéro de dossier administratif clinique</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.5.2" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Rang de naissance à renseigner si le patient est un  nouveau né</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.6.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Nom de famille du patient</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.6.2" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Prénom du patient</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.6.3" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Deuxième prénom du patient</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.6.4" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Sobriquet ou alias</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.6.5" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Civilité</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.6.6" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Diplôme</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.7.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Nom de jeune fille</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.8.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Date de naissance du patient</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.9.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Sexe (F, M ou U)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.10.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Race : INTERDIT</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.11.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Ligne 1 de l'adresse</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.11.2" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Ligne 2 de l'adresse</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.11.3" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Ville</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.11.4" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Libelllé département</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.11.5" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Code postal</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.11.6" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Pays</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.12.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Non utilisé</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.12.2" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Inutilisé</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.13.1" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Téléphone</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:sequence minOccurs="0" maxOccurs="unbounded">
					<xs:element name="P.14.1" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Code médecin, généralement l'ADELI</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="P.14.2" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Nom, prénom, etc médecin</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="P.14.3" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Type de code médecin</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="P.14.4" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Adresse médecin (optionnel)</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:element name="P.15.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Traitement local 1</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.16.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Traitement local 2</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.17.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Taille du patient, par défaut en centimètre</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.18.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Poids du patient, par défaut en kg</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.19.1" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Diagnostic connu ou suspecté relatif au patient</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.20.1" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Traitement : Nom des médicaments pris ou suspectés d'avoir été pris, separé par des répétiteurs</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.21.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Régime</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.22.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Commentaire 1 patient du demandeur</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.23.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Commentaire 2 patient du demandeur</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.24.1" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Date et heure d'admission ou de mouvement, éventuellement suivie de la date de sortie séparée par un répétiteur</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.25.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Statut de l'admission
OP : Sortie
IP : Entrée
IO : Entrée sortie (externe ou ambulatoire)
ER : Entrée urgence
MP : Mouvement interne ou modification
PA : Pré-admission</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.26.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Lit</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.26.2" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Chambre</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.26.3" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Code UF</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.26.4" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Libre, à définir localement si nécessaire</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.26.5" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Libre, à définir localement si nécessaire</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.26.6" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Libre, à définir localement si nécessaire</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.26.7" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Libre, à définir localement si nécessaire</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.26.8" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Libre, à définir localement si nécessaire</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.27.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Classification du diagnostic</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.28.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Religion : INTERDIT</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.29.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Situation maritale : 
M : Marié
S : Célibataire
D : Divorcé
W : Veuf
A : Séparé
U : Inconnu</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.30.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Précautions à prendre</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.31.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Langue</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.32.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Statut de confidentialité</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.33.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Date de derniere modification des données patient </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.34.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Date de décès</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="P.35.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Pour compatibilité ERA (vide)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="C" minOccurs="0"/>
				<xs:element ref="OBR" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AP" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="FAC" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="REG" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="L">
		<xs:annotation>
			<xs:documentation>Segment L : Fin de message</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="L.2.1">
					<xs:annotation>
						<xs:documentation>Rang du segment</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="L.3.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Non utilisé</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="L.4.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Nombre de segment P</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="L.5.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Nombre de segment du message</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="L.6.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Numéro de lot</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="C" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="OBR">
		<xs:annotation>
			<xs:documentation>Segment OBR : Demande d'analyses ou d'actes</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="OBR.2.1">
					<xs:annotation>
						<xs:documentation>Rang du segment</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="OBR.3.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Identifiiant de l'échantillon pour le demandeur</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OBR.3.2" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Identifiant de la demande pour le demandeur</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OBR.4.1" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Identifiant de l'échantillon pour l'executant</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OBR.4.2" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Identifiant de la demande pour l'executant</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:sequence maxOccurs="unbounded">
					<xs:element name="OBR.5.1">
						<xs:annotation>
							<xs:documentation>Demande d'analyse ou d'acte : code 1</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:minLength value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="OBR.5.2" minOccurs="0">
						<xs:annotation>
